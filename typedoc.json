{
    "codeBlocksDirectories": {
        "test": "./test/demo"
    },
    "entryPoints": [
        "src/index.ts"
    ],
    "includes": "./docs-source",
    "listInvalidSymbolLinks": true,
    "out": "docs",
    "pages": {
        "groups": [
            {
                "pages": [
                    {
                        "source": "./docs-source/guide/getting-started.md",
                        "title": "Getting started"
                    },
                    {
                        "source": "./docs-source/guide/use-with-guards.md",
                        "title": "Use with guards"
                    },
                    {
                        "source": "./docs-source/guide/better-type-constraints.md",
                        "title": "Better type constraints"
                    }
                ],
                "title": "Guides"
            }
        ]
    },
    "plugin": [
        "typedoc-plugin-pages-fork",
        "./docs-source/plugin"
    ],
    "theme": "pages-plugin",
    "tsconfig": "tsconfig.build.json"
}